<?xml version="1.0" encoding="utf-8"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <CubeBlocks>
        <Definition xsi:type="MyObjectBuilder_RadioAntennaDefinition">
            <Id>
                <TypeId>RadioAntenna</TypeId>
                <SubtypeId>LargeBlockRadioAntenna</SubtypeId>
            </Id>
            <DisplayName>Large Antenna Tower</DisplayName>
            <Components>
                <Component Subtype="SteelPlate" Count="20" />
                <Component Subtype="LargeTube" Count="10" />
                <Component Subtype="SmallTube" Count="20" />
                <Component Subtype="Construction" Count="50" />
				<Component Subtype="CopperWire" Count="100" />
				<Component Subtype="Electromagnet" Count="20" />
                <Component Subtype="RadioCommunication" Count="10" />
                <Component Subtype="Computer" Count="20" />
                <Component Subtype="SteelPlate" Count="20" />
            </Components>
            <CriticalComponent Subtype="Computer" Index="0" />
            <BlockPairName>Antenna</BlockPairName>
            <BuildTimeSeconds>40</BuildTimeSeconds>
			<MaxBroadcastPowerDrainkW>1</MaxBroadcastPowerDrainkW>
            <PCU>100</PCU>
        </Definition>
        <Definition xsi:type="MyObjectBuilder_BeaconDefinition">
            <Id>
                <TypeId>Beacon</TypeId>
                <SubtypeId>LargeBlockBeacon</SubtypeId>
            </Id>
            <DisplayName>Large Beacon</DisplayName>
			<Components>
				<Component Subtype="SteelPlate" Count="10" />
				<Component Subtype="LargeTube" Count="4" />
				<Component Subtype="CopperWire" Count="14" />
				<Component Subtype="Construction" Count="10" />
				<Component Subtype="Lightbulb" Count="4" />
				<Component Subtype="SteelPlate" Count="10" />
			</Components>
			<CriticalComponent Subtype="Lightbulb" Index="0" />
            <BlockPairName>Beacon</BlockPairName>            
            <BuildTimeSeconds>40</BuildTimeSeconds>
			<MaxBroadcastPowerDrainkW>1</MaxBroadcastPowerDrainkW>
            <PCU>50</PCU>
        </Definition>
        <Definition xsi:type="MyObjectBuilder_BeaconDefinition">
            <Id>
                <TypeId>Beacon</TypeId>
                <SubtypeId>SmallBlockBeacon</SubtypeId>
            </Id>
            <DisplayName>Small Beacon</DisplayName>
			<Components>
				<Component Subtype="SteelPlate" Count="1" />
				<Component Subtype="LargeTube" Count="1" />
				<Component Subtype="CopperWire" Count="4" />
				<Component Subtype="Construction" Count="1" />
				<Component Subtype="Lightbulb" Count="1" />
				<Component Subtype="SteelPlate" Count="1" />
			</Components>
			<CriticalComponent Subtype="Lightbulb" Index="0" />
            <BuildTimeSeconds>15</BuildTimeSeconds>
            <MaxBroadcastPowerDrainkW>1</MaxBroadcastPowerDrainkW>
            <PCU>50</PCU>
        </Definition>
        <Definition xsi:type="MyObjectBuilder_RadioAntennaDefinition">
            <Id>
                <TypeId>RadioAntenna</TypeId>
                <SubtypeId>SmallBlockRadioAntenna</SubtypeId>
            </Id>
            <DisplayName>DisplayName_Block_Antenna</DisplayName>
			<Components>
				<Component Subtype="SteelPlate" Count="1" />
				<Component Subtype="SmallTube" Count="2" />
				<Component Subtype="Construction" Count="5" />
				<Component Subtype="CopperWire" Count="10" />
				<Component Subtype="Electromagnet" Count="1" />
				<Component Subtype="Computer" Count="1" />
				<Component Subtype="SteelPlate" Count="1" />
			</Components>
			<CriticalComponent Subtype="Computer" Index="0" />
            <BlockPairName>Antenna</BlockPairName>           
            <BuildTimeSeconds>16</BuildTimeSeconds>
			<MaxBroadcastPowerDrainkW>1</MaxBroadcastPowerDrainkW>
            <PCU>100</PCU>
        </Definition>
        <Definition xsi:type="MyObjectBuilder_LaserAntennaDefinition">
            <Id>
                <TypeId>LaserAntenna</TypeId>
                <SubtypeId>LargeBlockLaserAntenna</SubtypeId>
            </Id>
            <DisplayName>Fast Laser Antenna Gimble</DisplayName>
            <Components>
                <Component Subtype="InteriorPlate" Count="20" />
				<Component Subtype="LargeTube" Count="4" />
				<Component Subtype="SmallTube" Count="6" />
                <Component Subtype="Construction" Count="10" />
				<Component Subtype="CopperWire" Count="20" />
                <Component Subtype="Electromagnet" Count="35" />
                <Component Subtype="Motor" Count="16" />
                <Component Subtype="RadioCommunication" Count="2" />
                <Component Subtype="Computer" Count="20" />
                <Component Subtype="SteelPlate" Count="20" />
            </Components>
            <CriticalComponent Subtype="Computer" Index="0" />           
            <BlockPairName>LaserAntenna</BlockPairName>            
            <BuildTimeSeconds>40</BuildTimeSeconds>                     
            <ResourceSinkGroup>Utility</ResourceSinkGroup>
            <PowerInputIdle>0.0001</PowerInputIdle>
            <PowerInputTurning>0.001</PowerInputTurning>
            <PowerInputLasing>1</PowerInputLasing>
            <RequireLineOfSight>true</RequireLineOfSight>            
            <PCU>100</PCU>
        </Definition>
        <Definition xsi:type="MyObjectBuilder_LaserAntennaDefinition">
            <Id>
                <TypeId>LaserAntenna</TypeId>
                <SubtypeId>SmallBlockLaserAntenna</SubtypeId>
            </Id>
            <DisplayName>Fast Laser Antenna Gimble</DisplayName>
			<Components>
				<Component Subtype="InteriorPlate" Count="4" />
				<Component Subtype="SmallTube" Count="8" />
				<Component Subtype="Construction" Count="10" />
				<Component Subtype="CopperWire" Count="20" />
				<Component Subtype="Electromagnet" Count="6" />
				<Component Subtype="Motor" Count="6" />
				<Component Subtype="RadioCommunication" Count="2" />
				<Component Subtype="Computer" Count="4" />
				<Component Subtype="SteelPlate" Count="10" />
			</Components>
			<CriticalComponent Subtype="Computer" Index="0" />
			<BlockPairName>LaserAntenna</BlockPairName>            
            <BuildTimeSeconds>20</BuildTimeSeconds>            
            <ResourceSinkGroup>Utility</ResourceSinkGroup>
            <PowerInputIdle>0.0001</PowerInputIdle>
            <PowerInputTurning>0.001</PowerInputTurning>
            <PowerInputLasing>1</PowerInputLasing> 
            <RequireLineOfSight>true</RequireLineOfSight>            
            <PCU>100</PCU>
        </Definition>
    </CubeBlocks>
</Definitions>
