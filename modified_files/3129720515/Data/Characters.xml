<?xml version="1.0" encoding="utf-8"?>
<Definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
<Characters>
<Character>
<Id>
<TypeId>MyObjectBuilder_Character</TypeId>
<SubtypeId>Default_Astronaut</SubtypeId>
</Id>
<Name>Default_Astronaut</Name>
<Mass>400</Mass>		<!--couldn't make the jetpack slower without it cutting off sound, so you're just heavier now :P-->
<EnableSpawnInventoryAsContainer>true</EnableSpawnInventoryAsContainer>
<InventorySpawnContainerId>
<TypeId>InventoryBagEntity</TypeId>
<SubtypeId>AstronautBackpack</SubtypeId>
</InventorySpawnContainerId>
<Inventory>
</Inventory>
<LootingTime>30</LootingTime>
<UseOnlyWalking>false</UseOnlyWalking>
<MaxSlope>60</MaxSlope>				<!--can't walk up steep slopes, might change this if it's too annoying...-->
<MaxSprintSpeed>6</MaxSprintSpeed>	<!--what if you were slower...-->
<MaxRunSpeed>3.5</MaxRunSpeed>
<MaxBackrunSpeed>3.3</MaxBackrunSpeed>
<MaxRunStrafingSpeed>3.4</MaxRunStrafingSpeed>
<MaxWalkSpeed>2</MaxWalkSpeed>
<MaxBackwalkSpeed>1.8</MaxBackwalkSpeed>
<MaxWalkStrafingSpeed>1.9</MaxWalkStrafingSpeed>
<MaxCrouchWalkSpeed>1.5</MaxCrouchWalkSpeed>
<MaxCrouchBackwalkSpeed>1.3</MaxCrouchBackwalkSpeed>
<MaxCrouchStrafingSpeed>1.4</MaxCrouchStrafingSpeed>
<JumpForce>2.5</JumpForce>			<!--doesn't seem to do anything??-->
<CharacterHeadSize>0.4</CharacterHeadSize>
<!-- <CharacterHeadSize>0.45</CharacterHeadSize>
<OxygenConsumption>0.063</OxygenConsumption>
<OxygenConsumptionMultiplier>3</OxygenConsumptionMultiplier>	<!-- >:D -->
<OxygenSuitRefillTime>25000</OxygenSuitRefillTime>				<!--slower suit fill in pressurized area-->
<MinOxygenLevelForSuitRefill>1</MinOxygenLevelForSuitRefill>	<!--raised to 1 to hopefully stop it from refilling in earthlike atmosphere-->
<PressureLevelForLowDamage>0.5</PressureLevelForLowDamage>
<DamageAmountAtZeroPressure>7</DamageAmountAtZeroPressure>
<NeedsOxygen>false</NeedsOxygen>
<SuitResourceStorage>
<Resource>
<Id>
<TypeId>GasProperties</TypeId>
<SubtypeId>Hydrogen</SubtypeId>
</Id>
<MaxCapacity>0.0001</MaxCapacity>		<!--so you don't get "fuel critical" constantly-->
<Throughput>1000</Throughput>
</Resource>
<Resource>
<Id>
<TypeId>GasProperties</TypeId>
<SubtypeId>Oxygen</SubtypeId>
</Id>
<Throughput>1000</Throughput>
<MaxCapacity>60</MaxCapacity>
</Resource>
</SuitResourceStorage>
<SuitConsumptionInTemperatureExtreme>25</SuitConsumptionInTemperatureExtreme>	<!--get indoors NOW-->
<Jetpack>
<Thrust>
<SideFlameOffset>0.00001</SideFlameOffset>		<!--puts the thruster lights closer to the middle, to get the pretty lights-->
<FrontFlameOffset>0.00001</FrontFlameOffset>
</Thrust>
<Thrust>
<SideFlameOffset>0.00001</SideFlameOffset>
<FrontFlameOffset>0.00001</FrontFlameOffset>
</Thrust>
<ThrustProperties>
<ResourceSinkGroup>Thrust</ResourceSinkGroup>
<ForceMagnitude>225</ForceMagnitude>				<!--any lower and it won't do the jetpack sounds-->
<SlowdownFactor>1</SlowdownFactor>					<!--dampeners shouldn't be more powerful than accelerating-->
<MinPowerConsumption>0.1</MinPowerConsumption>
<MaxPowerConsumption>1</MaxPowerConsumption>
<MinPlanetaryInfluence>0</MinPlanetaryInfluence>
<EffectivenessAtMinInfluence>1</EffectivenessAtMinInfluence>
<FuelConverter>
<FuelId>
<TypeId>GasProperties</TypeId>
<SubtypeId>Oxygen</SubtypeId>			<!--what if we just vent the oxygen for thrust >:] -->
</FuelId>									<!--basically what this mod started out as-->
<Efficiency>500000</Efficiency>				<!--otherwise minPwrConsumption is so small it creates rounding errors??-->
</FuelConverter>
</ThrustProperties>
</Jetpack>
<Name>Head</Name>
<Name>HelmetGlass</Name>
<Name>LeftHand</Name>
<Name>RightHand</Name>
<Name>LeftFingers</Name>
<Name>RightFingers</Name>
<Name>Spine</Name>
<Name>10</Name>
<Name>Ragdoll_SE_rig_Pelvis001</Name>
<Name>Ragdoll_SE_rig_Spine001</Name>
<Name>Ragdoll_SE_rig_Ribcage001</Name>
<Name>Ragdoll_SE_rig_LThigh001</Name>
<Name>Ragdoll_SE_rig_LCalf001</Name>
<Name>Ragdoll_SE_rig_LFoot001</Name>
<Name>Ragdoll_SE_rig_RThigh001</Name>
<Name>Ragdoll_SE_rig_RCalf001</Name>
<Name>Ragdoll_SE_rig_RFoot001</Name>
<Name>Ragdoll_SE_rig_LUpperarm001</Name>
<Name>Ragdoll_SE_rig_LForearm001</Name>
<Name>Ragdoll_SE_rig_RUpperarm001</Name>
<Name>Ragdoll_SE_rig_RForearm001</Name>
<Name>Ragdoll_SE_rig_RPalm001</Name>
<Name>Ragdoll_SE_rig_LPalm001</Name>
<Name>Ragdoll_SE_rig_Neck001</Name>
<Name>Ragdoll_SE_rig_Oxygentank_bone001</Name>
<Name>Jetpack</Name>
<Name>Jetpack_Weapon</Name>
<RecoilJetpackDampeningDegPerS>30</RecoilJetpackDampeningDegPerS>
